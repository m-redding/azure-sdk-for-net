<Project>
  <!--
    xunit and BouncyCastle are only approved for legacy use. Azure SDK test projects should use NUnit.
    Legacy Batch tests require ADAL and specific MSAL versions
  -->
  <ItemGroup Condition="'$(IsTestProject)' == 'true' or '$(IsTestSupportProject)' == 'true'">
    <PackageVersion Include="Microsoft.IdentityModel.Clients.ActiveDirectory" Version="[3.14.2, 4.0.0)" />
    <PackageVersion Update="Microsoft.Identity.Client" Version="4.61.3" />
    <PackageVersion Include="Portable.BouncyCastle" Version="1.9.0" />
    <PackageVersion Include="xunit" Version="2.4.2" />
    <PackageVersion Include="xunit.runner.visualstudio" Version="2.4.2" />
  </ItemGroup>
</Project>
