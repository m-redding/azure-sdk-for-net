<Project>
  <!--
    xunit and BouncyCastle are only approved for legacy use. Azure SDK test projects should use NUnit.
    Legacy Batch tests require ADAL and specific MSAL versions
  -->
  <ItemGroup Condition="'$(IsTestProject)' == 'true' or '$(IsTestSupportProject)' == 'true'">
    <PackageVersion Include="Microsoft.IdentityModel.Clients.ActiveDirectory" Version="$(MicrosoftIdentityModelClientsActiveDirectoryVersion)" />
    <PackageVersion Update="Microsoft.Identity.Client" Version="$(MicrosoftIdentityClientPinnedVersion)" />
    <PackageVersion Include="Portable.BouncyCastle" Version="$(PortableBouncyCastleVersion)" />
    <PackageVersion Include="xunit" Version="$(XunitVersion)" />
    <PackageVersion Include="xunit.runner.visualstudio" Version="$(XunitRunnerVersion)" />
  </ItemGroup>
</Project>
